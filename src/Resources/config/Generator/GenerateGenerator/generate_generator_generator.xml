<?xml version="1.0" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="open_classrooms.code_generator.entities.generate_generator_file_object_factory" class="OpenClassrooms\CodeGenerator\Entities\Impl\GenerateGeneratorFileObjectFactoryImpl" parent="open_classrooms.code_generator.file_objects.abstract_file_object_factory"/>
        <service id="open_classrooms.code_generator.skeleton_models.generate_generator.generate_generator_generator_skeleton_model_assembler" class="OpenClassrooms\CodeGenerator\SkeletonModels\GenerateGenerator\Impl\GenerateGeneratorGeneratorSkeletonModelAssemblerImpl">
            <call method="setBaseNamespace">
                <argument type="string">%base_namespace%</argument>
            </call>
        </service>
        <service id="symfony.component.filesystem.filesystem" class="Symfony\Component\Filesystem\Filesystem"/>
        <service id="open_classrooms.code_generator.repository.file_object_repository" class="OpenClassrooms\CodeGenerator\Repository\FileObjectRepository">
            <call method="setFileSystem">
                <argument type="service" id="symfony.component.filesystem.filesystem"/>
            </call>
        </service>

        <service id="open_classrooms.code_generator.generator.generate_generator.generate_generator_generator" class="OpenClassrooms\CodeGenerator\Generator\GenerateGenerator\GenerateGeneratorGenerator">
            <call method="setAssembler">
                <argument type="service" id="open_classrooms.code_generator.skeleton_models.generate_generator.generate_generator_generator_skeleton_model_assembler"/>
            </call>
            <call method="setFactory">
                <argument type="service" id="open_classrooms.code_generator.entities.generate_generator_file_object_factory"/>
            </call>
            <call method="setFileObjectGateway">
                <argument type="service" id="open_classrooms.code_generator.repository.file_object_repository"/>
            </call>
            <call method="setTemplating">
                <argument type="service" id="open_classrooms.code_generator.services.templating_service"/>
            </call>
        </service>

    </services>
</container>
