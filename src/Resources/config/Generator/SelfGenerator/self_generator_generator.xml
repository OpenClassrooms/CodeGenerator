<?xml version="1.0" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="open_classrooms.code_generator.entities.impl.self_generator_file_object_factory_impl" class="OpenClassrooms\CodeGenerator\Entities\Impl\SelfGeneratorFileObjectFactoryImpl"/>
        <service id="open_classrooms.code_generator.skeleton_models.self_generator.impl.self_generator_generator_skeleton_model_assembler_impl" class="OpenClassrooms\CodeGenerator\SkeletonModels\SelfGenerator\Impl\SelfGeneratorGeneratorSkeletonModelAssemblerImpl"/>
        <service id="symfony.component.filesystem.filesystem" class="Symfony\Component\Filesystem\Filesystem"/>
        <service id="open_classrooms.code_generator.repository.file_object_repository" class="OpenClassrooms\CodeGenerator\Repository\FileObjectRepository">
            <call method="setFileSystem">
                <argument type="service" id="symfony.component.filesystem.filesystem"/>
            </call>
        </service>

        <service id="open_classrooms.code_generator.generator.self_generator.self_generator_generator" class="OpenClassrooms\CodeGenerator\Generator\SelfGenerator\SelfGeneratorGenerator">
            <call method="setAssembler">
                <argument type="service" id="open_classrooms.code_generator.skeleton_models.self_generator.impl.self_generator_generator_skeleton_model_assembler_impl"/>
            </call>
            <call method="setFactory">
                <argument type="service" id="open_classrooms.code_generator.entities.impl.self_generator_file_object_factory_impl"/>
            </call>
            <call method="setFileObjectGateway">
                <argument type="service" id="open_classrooms.code_generator.repository.file_object_repository"/>
            </call>
            <call method="setTemplating">
                <argument type="service" id="open_classrooms.code_generator.services.impl.templating_service_impl"/>
            </call>
        </service>

    </services>
</container>
