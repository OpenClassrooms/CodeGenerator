<?xml version="1.0" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="open_classrooms.code_generator.file_objects.file_object_factory" class="OpenClassrooms\CodeGenerator\FileObjects\Impl\FileObjectFactoryImpl"/>
        <service id="open_classrooms.code_generator.generator.skeleton_models.view_models.view_model_test_detail_assembler" class="OpenClassrooms\CodeGenerator\Generator\SkeletonModels\ViewModels\Tests\Impl\ViewModelTestDetailAssemblerImpl"/>
        <service id="open_classrooms.code_generator.generator.skeleton_models.view_models.view_model.view_model_detail_assembler" class="OpenClassrooms\CodeGenerator\Generator\SkeletonModels\ViewModels\ViewModel\Impl\ViewModelDetailAssemblerImpl"/>

        <service id="open_classrooms.code_generator.gateway.file_object_gateway" class="OpenClassrooms\CodeGenerator\Gateway\Impl\FileObjectGatewayImpl">
        </service>

        <service id="open_classrooms.code_generator.generator.tests.api.view_models.view_model_test_generator" class="OpenClassrooms\CodeGenerator\Generator\Tests\Api\ViewModels\ViewModelTestGenerator">
            <call method="setFileObjectFactory">
                <argument type="service" id="open_classrooms.code_generator.file_objects.file_object_factory"/>
            </call>
            <call method="setFileObjectGateway">
                <argument type="service" id="open_classrooms.code_generator.gateway.file_object_gateway"/>
            </call>
            <call method="setViewModelTestDetailAssembler">
                <argument type="service" id="open_classrooms.code_generator.generator.skeleton_models.view_models.view_model_test_detail_assembler"/>
            </call>
        </service>

        <service id="open_classrooms.code_generator.generator.api.view_models.view_model_generator" class="OpenClassrooms\CodeGenerator\Generator\Api\ViewModels\ViewModelGenerator">
            <call method="setFileObjectFactory">
                <argument type="service" id="open_classrooms.code_generator.file_objects.file_object_factory"/>
            </call>
            <call method="setFileObjectGateway">
                <argument type="service" id="open_classrooms.code_generator.gateway.file_object_gateway"/>
            </call>
            <call method="setViewModelDetailAssembler">
                <argument type="service" id="open_classrooms.code_generator.generator.skeleton_models.view_models.view_model.view_model_detail_assembler"/>
            </call>
        </service>

    </services>
</container>
